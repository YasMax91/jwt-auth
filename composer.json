{
  "name": "ra-devs/jwt-auth",
  "description": "Universal JWT auth for Laravel (tymon/jwt-auth + ra-devs/api-json-response)",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "RaDevs(Max Yastremskyi)",
      "email": "m.yastremskyj@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "RaDevs\\JwtAuth\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "RaDevs\\JwtAuth\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "RaDevs\\JwtAuth\\Providers\\JwtAuthServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": ">=10.0",
    "illuminate/http": ">=10.0",
    "illuminate/notifications": ">=10.0",
    "illuminate/validation": ">=10.0",
    "illuminate/database": ">=10.0",
    "illuminate/mail": ">=10.0",
    "tymon/jwt-auth": "^2.0",
    "ra-devs/api-json-response": "^1.0"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}