{
  "name": "ra-devs/api-json-response",
  "description": "Consistent JSON API responses for Laravel",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "RaDevs\\ApiJsonResponse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RaDevs\\ApiJsonResponse\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "RaDevs\\ApiJsonResponse\\ApiJsonResponseServiceProvider"
      ],
      "aliases": {
        "ApiJsonResponse": "RaDevs\\ApiJsonResponse\\Facades\\ApiJsonResponse"
      }
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
